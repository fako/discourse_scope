{
    "name": "discourse-scope",
    "version": "0.3.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "integrate": "vue-cli-service serve --mode=integration",
        "build": "rm -rf dist/${npm_package_version} && vue-cli-service build --dest=.tmp && cp package.json .tmp/ && mkdir -p dist && mv .tmp/ dist/${npm_package_version}",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "1.2.7",
        "@fortawesome/free-solid-svg-icons": "5.4.2",
        "@fortawesome/vue-fontawesome": "0.1.2",
        "@sentry/browser": "5.15.5",
        "@sentry/integrations": "5.15.5",
        "axios": "0.19.0",
        "i": "0.3.6",
        "lodash": "4.17.15",
        "npm": "6.13.6",
        "onsenui": "2.10.6",
        "url-parse": "1.4.3",
        "vue": "2.5.17",
        "vue-i18n": "8.1.0",
        "vue-inject": "2.1.1",
        "vue-onsenui": "2.6.1",
        "vue-router": "3.0.1",
        "vue-select": "2.5.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "3.10.0",
        "@vue/cli-plugin-eslint": "3.0.4",
        "@vue/cli-service": "3.10.0",
        "moxios": "0.4.0",
        "node-sass": "4.13.1",
        "portfinder": "1.0.21",
        "sass-loader": "7.1.0",
        "vue-template-compiler": "2.5.17"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
